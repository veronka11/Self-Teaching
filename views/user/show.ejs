<script>
  var clientJsonVar = <%- JSON.stringify(o.actualUser) %>
    console.log(clientJsonVar);
  console.log(<%- JSON.stringify(o) %>);
</script>

<div class="container" style="margin-top: 80px;">

  <div class="row">
    <div class="col-md-5" id="vyska">
      <h2 class="nadpis2">All Categories</h2>
      <a href="/category/new" class="btn btn-info" role="button">Create new Category</a>

      <!--Table for all categories of user and of all users with public = true (1) -->
      <table class="table table-condensed" style="margin-top: 30px;">
        <tr class="info">
          <td></td>
          <td><strong>User</strong></td>
          <td><strong>Category</strong></td>
          <td></td>
          <td></td>
        </tr>
        <% _.each(o.categories, function(categor) { %>
        <tr data-id="<%= o.userN.id %>" data-model="categories" class="active">
          <!--<div class="row" style="margin: 12px; "> -->
          <td><span class="glyphicon glyphicon-user" style="color:black; margin: 5px;" aria-hidden="true"></span></td>
          <td><%= o.userN.name %></td>
          <td><%= categor.name %></td>
          <td><span class="glyphicon glyphicon-edit" aria-hidden="true"></span></td>
          <td><a href="/user/show/<%= o.userN.id %>" class="btn btn-sm btn-primary">Show</a></td>
          <!-- </div> -->
        </tr>
        <% }) %>
      </table>

    </div>
    <div class="col-md-2" id="ciara"
         style="border-left-style: outset; margin-left: 15px; width: 8px; margin-top: 15px;"></div>
    <script>
      $(document).ready(function () {
        console.log("+++++++++++++");
        console.log(<% JSON.stringify(o.userN) %>);
        var b = $("#vyska").height();
        document.getElementById('ciara').style.height = b.toString() + "px";
      });
    </script>
    <div class="col-md-5">
      <h2 class="nadpis2">All tests</h2>
      <a href="/test/new" class="btn btn-info" role="button">Create new Test</a>

      <!--Table for all categories of user and of all users with public = true (1) -->
      <% if (Object.keys(o.tests).length > 0 ) { %>
      <table class="table table-condensed" style="margin-top: 30px;">
        <tr class="info">
          <td></td>
          <td><strong>User</strong></td>
          <td><strong>Test</strong></td>
          <td></td>
          <td></td>
        </tr>
        <% _.each(o.tests, function(tests) { %>
        <tr data-id="<%= o.userN.id %>" data-model="test" class="active">
          <!--<div class="row" style="margin: 12px; "> -->
          <td><span class="glyphicon glyphicon-user" style="color:black; margin: 5px;" aria-hidden="true"></span></td>
          <td><%= o.userN.name %></td>
          <td><%= tests.name %></td>
          <td><span class="glyphicon glyphicon-edit" aria-hidden="true"></span></td>
          <td><a href="/user/show/<%= o.userN.id %>" class="btn btn-sm btn-primary">Show</a></td>
          <!-- </div> -->
        </tr>
        <% }) %>
      </table>
      <% } else { %>
        <div class="alert alert-info" style="margin-top: 15px;">
          <strong>Info!</strong> You haven't created any test yet.
        </div>
      <% } %>
    </div>
  </div>
</div>
